   "source": [
    "# 2480 주사위 세개_조건문\n",
    "a, b, c = map(int, input().split())\n",
    "prize = 0\n",
    "\n",
    "if a==b and b==c:\n",
    "    prize = 10000 + a * 1000\n",
    "elif a==b or a==c:\n",
    "    prize = 1000 + a * 100\n",
    "elif b==c:\n",
    "    prize = 1000 + b * 100\n",
    "else:\n",
    "    prize = max(a, b, c) * 100\n",
    "\n",
    "print(prize)"
   ]
  }
